{
  "name": "umn-web-template-components-react",
  "version": "0.2.0",
  "description": "",
  "main": "build/bundle.js",
  "peerDependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "build:watch": "webpack --watch",
    "build": "webpack ",
    "start:dev-do-not-use": "webpack-dev-server --config dev/webpack.config.js",
    "build:dev": "webpack --watch --config dev/webpack.config.js",
    "clean": "rm -rf node_modules && rm -f package-lock.json && rm -rf build && npm i && npm run build ",
    "buildPackage": "npm run clean -- -p && npm pack",
    "test": "NODE_ENV=test jest",
    "test:update": "npm run test -- -u",
    "eslint": "esw ",
    "eslintDev": "esw --watch"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime": "7.5.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.9.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "react-test-renderer": "^16.11.0",
    "enzyme-to-json": "3.4.3",
    "styled-components": "^4.4.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-watch": "^6.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1"
  },
  "jest": {
    "collectCoverage": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageReporters": [
      "html",
      "json-summary"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/webpack.config.js",
      "!**/build/**",
      "!**/dev/**"
    ]
  },
  "dependencies": {}
}
